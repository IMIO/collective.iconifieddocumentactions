<?xml version="1.0"?>
<object>
  <!-- hide the default Plone list of litteral document actions -->
  <hidden manager="plone.abovecontent" skinname="*">
    <viewlet name="plonemeeting.documentactions" />
  </hidden>
  <!-- if we are using PloneMeeting, we hide the viewlet defined in it that do the same -->
  <hidden manager="plone.abovecontenttitle" skinname="*">
    <viewlet name="plone.abovecontenttitle.documentactions" />
  </hidden>
</object>
